{
    "name": "@open-react-hub/react-icons",
    "version": "1.0.8",
    "description": "Lightweight, customizable React icon library",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/esm/index.js",
    "files": [
        "dist"
    ],
    "keywords": [
        "react",
        "icons",
        "svg",
        "typescript"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/abass-dev/open-react-hub/tree/main/packages/react-icons"
    },
    "author": "Abass Dev",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsup src/index.tsx --format cjs,esm --dts --minify",
        "dev": "tsup src/index.tsx --format cjs,esm --dts --watch",
        "lint": "eslint src/**/*.{ts,tsx}",
        "clean": "rm -rf dist",
        "test": "jest",
        "prepublishOnly": "npm run build"
    },
    "peerDependencies": {
        "react": "^17.0.0 || ^18.0.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@types/prismjs": "^1.26.3",
        "mixpanel-browser": "^2.59.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.0.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "typescript": "^5.0.0",
        "tsup": "^8.0.0",
        "jest": "^29.0.0",
        "@testing-library/react": "^14.0.0",
        "@testing-library/jest-dom": "^6.0.0"
    }
}